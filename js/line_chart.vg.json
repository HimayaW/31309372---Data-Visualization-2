{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Total Complementary protection over time (2006 - 2020)",
    "data": {"url": "https://raw.githubusercontent.com/HimayaW/31309372---Data-Visualization-2/main/data/Asylum%20Decisions.csv"},
    "mark": "line",
    "encoding": {
      "x": {"field": "Year", "type": "ordinal"},
      "y": {"field": "Total_complementary_protection", "type": "quantitative"},
      "color": {"field": "Country_of_asylum", "type": "nominal"},
      "tooltip": [
        {"field": "Country_of_asylum", "type": "nominal", "title": "Country of Asylum"},
        {"field": "Total_complementary_protection", "type": "quantitative", "title": "Applicants with complementary protection"},
        {"field": "Year", "type": "ordinal", "title": "Year"}
      ]
    },
    "config": {}
  }
  