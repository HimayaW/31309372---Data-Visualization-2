{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title" : "Relationship between no of applicants and rejected applicants",
    "data": {
      "url": "https://raw.githubusercontent.com/HimayaW/31309372---Data-Visualization-2/main/data/Asylum%20Decisions.csv"
    },
    "mark": "point",
    "encoding": {
      "x": {
        "field": "Total_rejected_decisions",
        "type": "quantitative",
        "scale": {"zero": false},
        "title": "Rejected Applicants"
      },
      "y": {
        "field": "Total_accepted",
        "type": "quantitative",
        "scale": {"zero": false},
        "title": "Accepted Applicants"
      },
      "color": {"field": "Country_of_asylum", "type": "nominal",
      "title": "Country of Asylum"},
      "size": {"field": "Recognition_rate", "type": "quantitative"}
    },
    "config": {}
  }